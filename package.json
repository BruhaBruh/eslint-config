{
  "name": "@bruhabruh/eslint-config",
  "version": "1.3.4",
  "description": "BruhaBruh ESLint Config for projects",
  "author": "BruhaBruh",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "eslint",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/eslint-config"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "config",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "pnpm run lint:js && pnpm run lint:other",
    "lint:js": "eslint . --report-unused-disable-directives",
    "lint:other": "prettier --check '**/*.{md,yaml,yml,json}'",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-security": "^3.0.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.1"
  },
  "devDependencies": {
    "@bruhabruh/prettier-config": "^1.1.5",
    "@bruhabruh/tsconfig": "^1.0.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "overrides": {
    "eslint": "^9.22.0"
  },
  "nano-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  }
}
